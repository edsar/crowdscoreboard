<h1>Listing user_reported_statistics</h1>

<table>
  <tr>
    <th>Statistic type</th>
    <th>Game</th>
    <th>Team</th>
    <th>Player</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stats_log_entries.each do |user_reported_statistic| %>
  <tr>
    <td><%= user_reported_statistic.statistic_type_id %></td>
    <td><%= user_reported_statistic.game_id %></td>
    <td><%= user_reported_statistic.team_id %></td>
    <td><%= user_reported_statistic.player_id %></td>
    <td><%= user_reported_statistic.user_id %></td>
   
  </tr>
<% end %>

</table>

<br />

